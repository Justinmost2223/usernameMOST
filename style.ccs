body {
  font-family: Arial, sans-serif;
  text-align: center;
  background-color: #f8f8f8;
  color: #333;
}

header {
  background-color: #222;
  color: white;
  padding: 20px;
}

button {
  background-color: #4CAF50;
  color: white;
  border: none;
  padding: 10px 20px;
  margin-top: 20px;
  cursor: pointer;
}

button:hover {
  background-color: #45a049;
}
